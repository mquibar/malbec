	<parameter name="FECHA_INICIO" class="java.util.Date"/>
	<parameter name="FECHA_FIN" class="java.util.Date"/>
	<queryString>
		<![CDATA[SELECT
     caja."fechaempaque" AS caja_fechaempaque,
     caja."peso" AS caja_peso,
     empleado."codigo" AS empleado_codigo,
     persona."apellido" || ', ' || persona."nombre" AS emp_nombre,
     tipouva."nombre" AS tipouva_nombre
FROM
     "public"."empleado" empleado INNER JOIN "public"."caja" caja ON empleado."idempleado" = caja."empleado_idempleado"
     INNER JOIN "public"."persona" persona ON empleado."idempleado" = persona."id"
     INNER JOIN "public"."tipouva" tipouva ON caja."tipouva_id" = tipouva."id"
where
    caja."fechaempaque" between $P{FECHA_INICIO} and $P{FECHA_FIN}
order by
    caja.id;]]>
	</queryString>
